{
  "name": "vite-base",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --mode development --port 5100",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "build-only:dev": "cross-env WEB_BASE='/spa/' vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "jsonserve": "node src/jsonserver/server.cjs",
    "docs:dev": "vitepress dev docs/vitepress",
    "docs:build": "cross-env DOCS_BASE='/docs/' vitepress build docs/vitepress",
    "docs:preview": "vitepress preview docs/vitepress"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@types/lodash-es": "^4.17.12",
    "@vueuse/components": "^10.9.0",
    "@vueuse/core": "^10.9.0",
    "axios": "^1.6.8",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "flex-hybrid-app-scripts": "^0.1.2",
    "lodash-es": "^4.17.21",
    "loglevel": "^1.9.1",
    "mitt": "^3.0.1",
    "pinia": "^2.1.7",
    "qs": "^6.12.0",
    "sass": "^1.74.1",
    "shortid": "^2.2.16",
    "swiper": "^11.1.0",
    "vee-validate": "^4.14.7",
    "vue": "^3.4.21",
    "vue-i18n": "^9.10.2",
    "vue-router": "^4.3.0",
    "vue-skeletor": "^1.0.6",
    "vuetify": "^3.5.12",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.28",
    "@types/qs": "^6.9.14",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "eslint": "^8.49.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.17.0",
    "json-server": "^0.17.4",
    "npm-run-all2": "^6.1.2",
    "postcss": "^8.4.39",
    "postcss-preset-env": "^9.6.0",
    "prettier": "^3.2.5",
    "typescript": "~5.4.0",
    "vite": "^5.1.6",
    "vite-plugin-vue-devtools": "^7.0.18",
    "vitepress": "^1.1.0",
    "vue-tsc": "^2.0.6"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead"
  ]
}
